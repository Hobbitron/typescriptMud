{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "declaration": true,
        "outDir": "bin"
    },
    "exclude": [
        "node_modules",
        "typings",
        ".c9"
    ],
    "file": [
        "src/**/*.ts"
    ]
}